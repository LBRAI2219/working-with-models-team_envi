import random
r = random.random()
rd = r*30               #random deviation angle
Axiom: FL(0)I(0)

derivation length: 2
production:
L(x):                                #leaf (temporary)
  produce:[,(2)-(90)~l] [,(2)+(90)~l]
I(x):                                #Internode 
  if r <0.5:                         #random deviation of the stem
    produce -(rd)FI(x+1)
  else:
    produce +(rd)FI(x+1)



